{
  "name": "uxshot",
  "version": "0.0.22",
  "main": "dist/uxshot.js",
  "license": "MIT",
  "repository": "github:alanfriedman/uxshot",
  "scripts": {
    "build": "npx webpack",
    "build:production": "NODE_ENV=production yarn build",
    "prettify": "npx prettier --write"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/runtime": "^7.6.0",
    "babel-loader": "^8.0.6",
    "core-js": "2",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7"
  },
  "dependencies": {
    "hotkeys-js": "^3.7.1"
  },
  "browserslist": "> 0.25%, not dead",
  "lint-staged": {
    "*.{js,jsx}": [
      "yarn prettify",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged && yarn build && yarn build:production && git add dist"
    }
  }
}
